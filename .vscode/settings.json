{
    "git.autofetch": true,
    "eslint.format.enable": true,
    "eslint.run": "onSave",
    "eslint.trace.server": "off",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
        "source.organizeImports": true
    },
    "eslint.quiet": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "html",
        "typescriptreact"
    ]
}